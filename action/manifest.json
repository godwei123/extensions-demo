{
    "manifest_version": 3,
    "name": "Page Info",
    "version": "1.0.0",
    "description": "Shows current page information",
    "permissions": ["activeTab", "tabs"],
    "action": {
        "default_popup": "popup/popup.html",
        "default_icon": {
            "16": "icons/icon.svg",
            "48": "icons/icon.svg",
            "128": "icons/icon.svg"
        }
    },
    "icons": {
        "16": "icons/icon.svg",
        "48": "icons/icon.svg",
        "128": "icons/icon.svg"
    }
}
