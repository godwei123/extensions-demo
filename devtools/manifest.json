{
    "manifest_version": 3,
    "name": "Cookie Inspector",
    "version": "1.0.0",
    "description": "Display cookies in DevTools panel",
    "permissions": ["cookies"],
    "host_permissions": ["<all_urls>"],
    "devtools_page": "devtools/devtools.html",
    "icons": {
        "16": "icons/icon.svg",
        "48": "icons/icon.svg",
        "128": "icons/icon.svg"
    }
}
