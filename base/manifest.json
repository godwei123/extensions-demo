{
    "manifest_version": 3,
    "name": "My Chrome Extension",
    "version": "1.0.0",
    "description": "A Chrome extension template",
    "permissions": ["storage", "activeTab"],
    "action": {
        "default_popup": "popup/popup.html",
        "default_icon": {
            "16": "icons/icon.png",
            "48": "icons/icon.png",
            "128": "icons/icon.png"
        }
    },
    "background": {
        "service_worker": "background/background.js"
    },
    "icons": {
        "16": "icons/icon.png",
        "48": "icons/icon.png",
        "128": "icons/icon.png"
    }
}
